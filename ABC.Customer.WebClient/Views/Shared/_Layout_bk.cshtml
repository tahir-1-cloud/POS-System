        $(document).ready(function () {

            $("#searchdropdown").hide();
            $("#btnSearch").click(function () {
                $("#myInput").attr("hidden", false);
                //$("#select").attr("hidden", false);
            });

            $.ajax({
            url: '@Url.Action("GetEmployerJson", "Home",new{@area="Home"})',
            async: false,
            contentType: "application/json;",
            dataType: "json",
            success: function (data) {
                console.log(data);
              
                $.each(data, function (index, item) {
                    var html = '';
                    html += '<div class="cp" onclick="ItemSelect()">';
                    html += '<span class="item_name" data-rel=' + item.id + '>' + item.name + '</span>';
                    html += '</div>';
                    $("#searchdropdown").append(html);
                });
            },
            error: function (error) {
                alert(error);
            }
                  });

        });



        function GetAllMatchedMissionsText(event) {
            var naming = $("#myInput").val();
            console.log(naming);
            if (naming == "") {
                $(".item_name").parent().hide();
                $("#searchdropdown").hide();
            }
            else {
                console.log(naming);

                var text = naming;
                var items = $(".item_name");
                //first, hide all:
                items.parent().hide();

                //show only those matching user input:
                items.filter(function () {
                    return $(this).text().toLowerCase().indexOf(text) == 0;
                }).parent().show();
                $("#searchdropdown").show();
            }
        };

        function ItemSelect() {

        };

    </script>
