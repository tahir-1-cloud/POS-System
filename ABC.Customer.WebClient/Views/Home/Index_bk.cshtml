    <div class="container my-3">

        <br />
        <div class="card">
            <div class="card-body">
                <div class="row">
                    @{ var i = 0; }
                    @if (Model != null)
                    {
                        @foreach (var item in Model)
                        {
                            i = i + 1;

                            <div class="col-md-4">
                                @*<img src="~/images/india-ranked-5th-in-pictorial-warnings-on-cigarette-packets.jpg" height="120" width="150" alt="">*@

                                @if (item.ItemImageByPath != null && item.ItemImageByPath != "")
                                {
                                    <img src="@item.ItemImageByPath" height="120" width="150" alt="">
                                }
                                else
                                {
                                    <img src="~/images/india-ranked-5th-in-pictorial-warnings-on-cigarette-packets.jpg" height="120" width="150" alt="">
                                }
                                <h5 class="py-2">@item.Name</h5>
                                <div>
                                    <a href="@Url.Action("ProductDetail", "Home", new {@area = "Home", id = item.Id })" class="btn btn-primary">Add to Cart</a>
                                </div>
                            </div>
                        }
                    }
                </div>
            </div>
        </div>

    </div>



