        <div class="col-md-2  ml-5" style=" margin-top: -31.4em; ">
            <nav id="menu">
                <ul class="parent-menu overflow-auto">
                    <li>

                        @foreach (var item in ViewBag.ItemCategory)
                        {
                        <li>

                            <a href="#">@item.Name<span class="fas fa-chevron-right"></span></a>
                            @foreach (var subitem in ViewBag.ItemsubCategory)
                            {
                                if (subitem != null)
                                {
                                    @if (item.Id == subitem.CategoryId)
                                    {


                                        <ul>
                                            <li><a href="#">@subitem.SubCategory</a></li>
                                        </ul>

                                    }
                                }
                            }
                        </li>

                    }
                        </li>

                    </ul>
                </nav>
            </div>
